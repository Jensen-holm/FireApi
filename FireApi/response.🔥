from python import PythonObject


struct Response:
    var status_code: Int
    var body: String

    fn __init__(inout self, status_code: Int, body: String) raises -> None:
        self.status_code = status_code
        self.body = body

    fn to_bytes(borrowed self, py_builtins: PythonObject) raises -> PythonObject:
        return py_builtins.bytes(self.body)
