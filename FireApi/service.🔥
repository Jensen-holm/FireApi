from FireApi.response import Response
from FireApi.request import Request


@value
trait HTTPService(TCPService):
    fn func(self, req: Request) raises -> Response:
        ...

    @always_inline
    fn get_endpoint(self) -> String:
        ...

    @always_inline
    fn get_method(self) -> String:
        ...


@value
trait TCPService(Copyable):
    fn func (self, req: Request) raises -> Response:
        ...
