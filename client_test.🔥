from FireApi import Client, Response, Request
import random


fn main() raises -> None:
    let client = Client(
        host_name="127.0.0.1",
        port=9090,
        endpoint="/fibonnaci",
    )

    random.seed(1)
    let num = random.random_si64(min=20, max=50)
    let req = Request(
        body=str(num),
        method="GET",
    )

    let resp = client.send_request(request=req)
    print(resp.body)
