from FireApi import Client, Request, Response


fn main() raises -> None:
    let client = Client(
        host_name="127.0.0.1",
        port=8080,
        endpoint="/index",
    )

    let request = Request(
        body="hello world",
    )

    let response: Response = client.send_request(
        request=request,
    )

    print(response.body)
